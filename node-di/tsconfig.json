{
    "compilerOptions": {
      "module": "commonjs",
      "moduleResolution": "node",
      "target": "es6",
      "noImplicitAny": false,
      "allowSyntheticDefaultImports": true,
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
      "sourceMap": true,
      "outDir": "dist",
      "lib": [
        "es2015.promise",
        "es2017",
        "esnext.asynciterable",
        "dom"
      ],
      "baseUrl": ".",
      "paths": {
        "*": [
          "libs/*"
        ]
      }
    },
    "exclude": [
      "node_modules",
      "dist",
      "test",
      "**/*.spec.ts",
      "x-local"
    ]
  }
  